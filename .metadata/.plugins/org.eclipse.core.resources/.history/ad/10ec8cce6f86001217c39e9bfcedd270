db.messages.aggregate([ 
	{project:{"headers.From":1,"headers.To":1}}, 
	{unwind:"headers.To"}, 
	{group:
		{"_id":"headers.From", "ToArray":{addToSet:"headers.To"}}
	},
	{unwind:"ToArray"}, 
	{group:
		{"_id":"_id.headers.From","To":"$ToArray"}
	}
])


