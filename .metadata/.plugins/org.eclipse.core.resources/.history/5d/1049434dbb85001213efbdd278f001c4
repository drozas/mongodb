
import pymongo
import sys

def find():
    # establish a connection to the database
    connection = pymongo.Connection("mongodb://localhost", safe=True)
    # get a handle to the school database
    db=connection.test
    images = db.images
    albums = db.albums

    print "find, reporting for duty"
    try:
        for album in albums:
            print albums

        print images
        for image in images:
            print image
    except:
        print "Unexpected error:", sys.exc_info()[0]

find()